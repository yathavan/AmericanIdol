    		<version>1.0</version>
     		<systemPath>${basedir}\libs\djl-basicdataset-0.1.0.jar</systemPath>
        </dependency>
		<dependency>
     		<groupId>djl-model-zoo</groupId>
     		<artifactId>djl-model-zoo</artifactId>
     		<scope>system</scope>
    		<version>1.0</version>
     		<systemPath>${basedir}\libs\djl-model-zoo-0.1.0.jar</systemPath>
