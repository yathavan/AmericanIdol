        </dependency>
        <dependency>
            <groupId>djl-mxnet-engine</groupId>
            <artifactId>djl-mxnet-engine</artifactId>
			<scope>system</scope>
    		<version>1.0</version>
     		<systemPath>${basedir}\libs\djl-mxnet-engine-0.1.0.jar</systemPath>
        </dependency>
        <!-- Pytorch -->
        <dependency>
            <groupId>pytorch-engine</groupId>
            <artifactId>pytorch-engine</artifactId>
			<scope>system</scope>
    		<version>1.0</version>
     		<systemPath>${basedir}\libs\pytorch-engine-0.1.0.jar</systemPath>
        </dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

</project>
